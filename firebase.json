{
  "hosting": {
    "public": "public",
    "redirects": [
      {
        "source": "/",
        "destination": "/about.html",
        "type": 301
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
